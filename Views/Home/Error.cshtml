@{
    Layout = null;
}
@model IList<string>

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Error</title>
</head>
<body>
    <table>
        @{
           List<string> data = ViewBag.Name ;
           List<string> path=ViewBag.Path;
           for (int i = 0; i < data.Count;i++ )
           {
                <tr>
                <td>Folder</td>
                <td>@Html.ActionLink(data[i], "data", "Home", new { value = data[i],path=path[i] }, null)</td>
            </tr>
           }
        }
        
    </table>

</body>
</html>